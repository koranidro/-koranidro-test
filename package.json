    "pub:major": "npm version major && npm publish --access=public",
    "dev": "bun index.ts"
  },
  "devDependencies": {
    "@koranidro/test": "^0.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-terser": "^0.4.4",
